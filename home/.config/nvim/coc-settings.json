{
  "list.normalMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit",
    "<C-s>": "action:split",
    "<C-j>": "do:next",
    "<C-k>": "do:previous"
  },
  "diagnostic.warningSign": "!",
  "diagnostic.checkCurrentLine": true,
  "[go]": {
    "coc.preferences.formatOnSave": true,
  },
  "coc.source.tag.filetypes": ["ruby"]
}
