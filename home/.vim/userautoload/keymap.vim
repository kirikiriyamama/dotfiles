set backspace=indent,eol,start

nnoremap ZZ <Nop>
nnoremap ZQ <Nop>
nnoremap Q  <Nop>
nnoremap K  <Nop>

nnoremap ; :

noremap  <C-c> <Nop>
inoremap <C-c> <Nop>

nnoremap <C-w>c :tabnew<CR>
nnoremap <C-w>n gt
nnoremap <C-w>p gT

nnoremap <C-]> g<C-]>

cnoremap <C-p> <Up>
cnoremap <C-n> <Down>
cnoremap <C-b> <Left>
cnoremap <C-f> <Right>
cnoremap <C-a> <Home>
cnoremap <C-e> <End>
cnoremap <C-d> <Del>
